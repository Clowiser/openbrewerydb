  },
  "homepage": "https://github.com/openbrewerydb/openbrewerydb#readme",
  "dependencies": {
    "csvtojson": "^2.0.10",
    "glob": "^7.1.6",
    "json-format": "^1.0.1",
    "slugify": "^1.3.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
